{
  "name": "anothermonkey",
  "displayName": "AnotherMonkey",
  "version": "0.0.1",
  "description": "A modern userscript manager built with Plasmo and Manifest V3.",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "@monaco-editor/react": "^4.7.0",
    "@plasmohq/messaging": "^0.7.2",
    "@plasmohq/storage": "^1.15.0",
    "clsx": "^2.1.1",
    "dexie": "^4.2.1",
    "dexie-react-hooks": "^4.2.0",
    "lucide-react": "^0.563.0",
    "monaco-editor": "^0.55.1",
    "plasmo": "^0.90.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@types/chrome": "^0.1.36",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.23",
    "postcss": "^8.5.6",
    "prettier": "^3.8.1",
    "tailwindcss": "^3.4.19",
    "typescript": "^5.9.3"
  },
  "manifest": {
    "permissions": [
      "userScripts",
      "storage",
      "unlimitedStorage",
      "tabs",
      "scripting",
      "activeTab",
      "notifications",
      "identity",
      "downloads"
    ],
    "optional_permissions": [
      "<all_urls>"
    ],
    "host_permissions": [
      "https://*/*",
      "http://*/*",
      "file://*/*"
    ]
  }
}
